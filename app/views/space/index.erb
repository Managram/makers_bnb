<h1> Spaces </h1>

<form action="/space/new">
  <button type="submit">List a Space</button>
</form>

<ul id='spaces'>
  <% @spaces.each do |space| %>
    <li>
        <form action="/space/<%= space.id %>/view">
          Space: <%= space.name %>
          <input type="submit" value="view space">
        </form>
    </li>
  <% end %>
</ul>
